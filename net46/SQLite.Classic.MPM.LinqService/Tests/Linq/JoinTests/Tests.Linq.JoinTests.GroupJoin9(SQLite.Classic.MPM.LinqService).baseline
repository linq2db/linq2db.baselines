-- SQLite.Classic.MPM SQLite.Classic SQLite

SELECT
	[y_4].[ParentID],
	[y_4].[Value1],
	[t1].[ParentID],
	[t1].[z],
	[t1].[is_empty],
	[yid].[ParentID],
	[yid].[Value1],
	[y].[ParentID],
	[y].[Value1],
	[y_1].[ParentID],
	[y_1].[Value1],
	[y_3].[ParentID],
	[y_3].[Value1]
FROM
	[Parent] [y_4]
		LEFT JOIN [Parent] [yid] ON [y_4].[ParentID] = [yid].[ParentID]
		LEFT JOIN [Parent] [y] ON [y_4].[ParentID] = [y].[ParentID]
		LEFT JOIN [Parent] [y_1] ON [y_4].[ParentID] = [y_1].[ParentID]
		LEFT JOIN (
			SELECT
				[y_2].[Value1] as [z],
				1 as [is_empty],
				[y_2].[ParentID]
			FROM
				[Parent] [y_2]
		) [t1] ON [y_4].[ParentID] = Coalesce([t1].[z], 1)
		LEFT JOIN [Parent] [y_3] ON [y_4].[ParentID] = [y_3].[ParentID]

