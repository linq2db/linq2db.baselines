-- Oracle.23.Managed Oracle.Managed Oracle12

SELECT
	CASE
		WHEN MOD(p."ParentID", 2) = 0 THEN CASE
			WHEN MOD(p."ParentID", 3) = 0 THEN 1
			ELSE 0
		END
		WHEN MOD(p."ParentID", 4) = 0 THEN CASE
			WHEN p."ParentID" > 0 THEN 1
			ELSE 0
		END
		ELSE CASE
			WHEN p."ParentID" < 5 THEN 1
			ELSE 0
		END
	END
FROM
	"Parent" p

