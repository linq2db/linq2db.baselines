-- MySqlConnector MySql
DECLARE @DateTimeValue_1 Datetime -- DateTime
SET     @DateTimeValue_1 = '2009-01-01'

SELECT
	`_`.`BinaryValue`,
	`_`.`BoolValue`,
	`_`.`DateTimeValue`,
	`_`.`GuidValue`,
	`_`.`ID`,
	`_`.`MoneyValue`,
	`_`.`SmallIntValue`,
	`_`.`StringValue`
FROM
	`LinqDataTypes` `_`
WHERE
	`_`.`DateTimeValue` > @DateTimeValue_1

