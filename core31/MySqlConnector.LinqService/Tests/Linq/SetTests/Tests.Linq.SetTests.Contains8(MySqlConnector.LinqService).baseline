SELECT
	`p`.`ParentID`,
	`p`.`Value1`
FROM
	`Parent` `p`
		INNER JOIN `Child` `ch` ON `p`.`ParentID` = `ch`.`ParentID`
		INNER JOIN `GrandChild` `gc` ON `ch`.`ChildID` = `gc`.`ChildID`
WHERE
	(`gc`.`ChildID` = 11 AND `gc`.`GrandChildID` = 111 AND `gc`.`ParentID` = 1 OR `gc`.`ChildID` = 21 AND `gc`.`GrandChildID` = 211 AND `gc`.`ParentID` = 2)

