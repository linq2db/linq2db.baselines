-- MySqlConnector MySql

SELECT
	`_`.`BigIntValue`,
	`_`.`BoolValue`,
	`_`.`DateTimeValue`,
	`_`.`DateTimeValue2`,
	`_`.`GuidValue`,
	`_`.`ID`,
	`_`.`IntValue`,
	`_`.`MoneyValue`,
	`_`.`SmallIntValue`,
	`_`.`StringValue`
FROM
	`LinqDataTypes` `_`
WHERE
	(CASE
		WHEN `_`.`BoolValue` IN (1)
			THEN 1
		ELSE 0
	END = 1 AND CASE
		WHEN `_`.`BoolValue` IN (1)
			THEN 1
		ELSE 0
	END IS NOT NULL)

