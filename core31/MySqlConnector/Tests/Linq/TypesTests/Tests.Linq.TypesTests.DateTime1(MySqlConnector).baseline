-- MySqlConnector MySql
DECLARE @Date_1 Datetime -- DateTime
SET     @Date_1 = '2009-09-20'

SELECT
	`t`.`BigIntValue`,
	`t`.`BoolValue`,
	`t`.`DateTimeValue`,
	`t`.`DateTimeValue2`,
	`t`.`GuidValue`,
	`t`.`ID`,
	`t`.`IntValue`,
	`t`.`MoneyValue`,
	`t`.`SmallIntValue`,
	`t`.`StringValue`
FROM
	`LinqDataTypes` `t`
WHERE
	Cast(`t`.`DateTimeValue` as Date) > @Date_1

