-- MySqlConnector MySql
DECLARE @s VarChar(7) -- String
SET     @s = '123[456'
DECLARE @ps VarChar(3) -- String
SET     @ps = '%[%'

SELECT
	`p`.`FirstName`,
	`p`.`Gender`,
	`p`.`PersonID`,
	`p`.`LastName`,
	`p`.`MiddleName`
FROM
	`Person` `p`
WHERE
	`p`.`PersonID` = 1 AND @s LIKE @ps ESCAPE '~'

