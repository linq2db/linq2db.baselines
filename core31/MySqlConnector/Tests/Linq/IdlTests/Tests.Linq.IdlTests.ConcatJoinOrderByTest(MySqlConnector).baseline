-- MySqlConnector MySql

SELECT
	`person_1`.`PersonID`,
	`y`.`PersonID`
FROM
	(
		SELECT
			`pat`.`Diagnosis`,
			`pat`.`PersonID`
		FROM
			`Patient` `pat`
		WHERE
			`pat`.`Diagnosis` = 'a'
		UNION ALL
		SELECT
			`pat_1`.`Diagnosis`,
			`pat_1`.`PersonID`
		FROM
			`Patient` `pat_1`
		WHERE
			`pat_1`.`Diagnosis` = 'b'
	) `y`
		INNER JOIN `Person` `person_1` ON `y`.`PersonID` = `person_1`.`PersonID`
ORDER BY
	`person_1`.`PersonID`

