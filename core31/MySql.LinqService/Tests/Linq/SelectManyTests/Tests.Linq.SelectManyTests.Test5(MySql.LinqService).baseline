SELECT
	`t1`.`ChildID`,
	`t1`.`ParentID`
FROM
	(
		SELECT
			`a_Child`.`ChildID`,
			`a_Child`.`ParentID`
		FROM
			`Parent` `p`
				CROSS JOIN `GrandChild` `g_1`
				LEFT JOIN `Child` `a_Child` ON `g_1`.`ParentID` = `a_Child`.`ParentID` AND `g_1`.`ChildID` = `a_Child`.`ChildID`
	) `t1`,
	`Parent` `c_1`

