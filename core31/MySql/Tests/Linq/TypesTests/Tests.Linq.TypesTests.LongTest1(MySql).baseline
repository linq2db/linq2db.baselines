DECLARE @value_1 Int64
SET     @value_1 = 0

SELECT
	`t`.`BigIntValue`,
	`t`.`BoolValue`,
	`t`.`DateTimeValue`,
	`t`.`DateTimeValue2`,
	`t`.`GuidValue`,
	`t`.`ID`,
	`t`.`IntValue`,
	`t`.`MoneyValue`,
	`t`.`SmallIntValue`,
	`t`.`StringValue`
FROM
	`LinqDataTypes` `t`
WHERE
	`t`.`BigIntValue` = @value_1

