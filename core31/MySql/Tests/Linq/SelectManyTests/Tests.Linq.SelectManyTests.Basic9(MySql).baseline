SELECT
	`t1`.`ChildID`,
	`t1`.`GrandChildID`,
	`t1`.`ParentID`
FROM
	`Parent` `cp_1`
		INNER JOIN (
			SELECT
				`c_1`.`ChildID`,
				`c_1`.`GrandChildID`,
				`c_1`.`ParentID`,
				`cp`.`ParentID` as `ParentID_1`
			FROM
				`Child` `cp`,
				`GrandChild` `c_1`
		) `t1` ON `cp_1`.`ParentID` = `t1`.`ParentID_1` AND `cp_1`.`ParentID` = `t1`.`ParentID`

