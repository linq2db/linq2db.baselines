-- MySql MySql.Official MySql

SELECT
	`p_1`.`FirstName`,
	`p_1`.`Gender`,
	`p_1`.`PersonID`,
	`p_1`.`LastName`,
	`p_1`.`MiddleName`
FROM
	(
		SELECT
			CASE
				WHEN `p`.`FirstName` LIKE '%Jo%'
					THEN 1
				ELSE 0
			END as `Field1`,
			`p`.`FirstName`,
			`p`.`Gender`,
			`p`.`PersonID`,
			`p`.`LastName`,
			`p`.`MiddleName`
		FROM
			`Person` `p`
	) `p_1`
WHERE
	(`p_1`.`Field1` = 1 AND `p_1`.`Field1` IS NOT NULL)
ORDER BY
	`p_1`.`Field1`

