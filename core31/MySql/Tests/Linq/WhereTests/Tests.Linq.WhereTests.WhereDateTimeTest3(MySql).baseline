DECLARE @DateTimeValue_1 DateTime
SET     @DateTimeValue_1 = '2009-09-27'

SELECT
	`_`.`BinaryValue`,
	`_`.`BoolValue`,
	`_`.`DateTimeValue`,
	`_`.`GuidValue`,
	`_`.`ID`,
	`_`.`MoneyValue`,
	`_`.`SmallIntValue`,
	`_`.`StringValue`
FROM
	`LinqDataTypes` `_`
WHERE
	`_`.`DateTimeValue` = @DateTimeValue_1

