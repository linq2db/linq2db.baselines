-- PostgreSQL PostgreSQL.9.5 PostgreSQL

WITH RECURSIVE "MY_CTE" ("ChildID", "GrandChildID", "ParentID")
AS
(
	SELECT
		gc1."ChildID",
		gc1."GrandChildID",
		gc1."GrandChildID"
	FROM
		"GrandChild" gc1
	UNION ALL
	SELECT
		ct."ChildID",
		ct."ChildID" + 1,
		ct."ParentID"
	FROM
		"GrandChild" gc
			INNER JOIN "Parent" p ON p."ParentID" = gc."ParentID"
			INNER JOIN "MY_CTE" ct ON ct."ChildID" = gc."ChildID"
	WHERE
		ct."GrandChildID" <= 10
)
SELECT
	t1."ChildID",
	t1."GrandChildID",
	t1."ParentID"
FROM
	"MY_CTE" t1

