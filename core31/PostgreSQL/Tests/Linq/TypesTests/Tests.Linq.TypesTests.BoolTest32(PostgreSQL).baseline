-- PostgreSQL PostgreSQL.9.5 PostgreSQL

SELECT
	t."BigIntValue",
	t."BoolValue",
	t."DateTimeValue",
	t."DateTimeValue2",
	t."GuidValue",
	t."ID",
	t."IntValue",
	t."MoneyValue",
	t."SmallIntValue",
	t."StringValue"
FROM
	"LinqDataTypes" t
WHERE
	(t."BoolValue" = True AND t."BoolValue" IS NOT NULL)

