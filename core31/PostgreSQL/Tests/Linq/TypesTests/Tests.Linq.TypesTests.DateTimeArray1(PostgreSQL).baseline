-- PostgreSQL PostgreSQL.9.5 PostgreSQL
DECLARE @p1 Timestamp -- DateTime
SET     @p1 = '2001-01-11 01:11:21.100'::timestamp

SELECT
	t."BigIntValue",
	t."BoolValue",
	t."DateTimeValue",
	t."DateTimeValue2",
	t."GuidValue",
	t."ID",
	t."IntValue",
	t."MoneyValue",
	t."SmallIntValue",
	t."StringValue"
FROM
	"LinqDataTypes" t
WHERE
	t."DateTimeValue" IN (:p1)

