-- PostgreSQL PostgreSQL.9.5 PostgreSQL
DECLARE @take Integer -- Int32
SET     @take = 1

SELECT
	t2."ChildID",
	t2."ParentID"
FROM
	"Parent" p
		LEFT JOIN LATERAL (
			SELECT
				t1."ChildID",
				t1."ParentID"
			FROM
				"Child" t1
			WHERE
				p."ParentID" = t1."ParentID"
			ORDER BY
				t1."ChildID"
			LIMIT :take
		) t2 ON 1=1

