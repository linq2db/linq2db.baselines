-- PostgreSQL PostgreSQL.9.5 PostgreSQL
DECLARE @take Integer -- Int32
SET     @take = 2

SELECT
	t."BinaryValue",
	t."BoolValue",
	t."DateTimeValue",
	t."GuidValue",
	t."ID",
	t."MoneyValue",
	t."SmallIntValue",
	t."StringValue"
FROM
	"LinqDataTypes" t
WHERE
	t."MoneyValue" * Cast(t."ID" as Decimal(29,10)) = 1.11
LIMIT :take

