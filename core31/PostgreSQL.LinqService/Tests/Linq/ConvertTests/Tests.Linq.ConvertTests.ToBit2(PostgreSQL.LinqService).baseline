-- PostgreSQL PostgreSQL.9.5 PostgreSQL

SELECT
	t."BinaryValue",
	t."BoolValue",
	t."DateTimeValue",
	t."GuidValue",
	t."ID",
	t."MoneyValue",
	t."SmallIntValue",
	t."StringValue"
FROM
	"LinqDataTypes" t
WHERE
	(CASE
		WHEN t."MoneyValue" - 4.5 = 0
			THEN False
		ELSE True
	END = False OR CASE
		WHEN t."MoneyValue" - 4.5 = 0
			THEN False
		ELSE True
	END IS NULL)

