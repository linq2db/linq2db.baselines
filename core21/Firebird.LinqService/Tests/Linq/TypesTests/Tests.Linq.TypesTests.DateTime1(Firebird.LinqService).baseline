-- Firebird
DECLARE @Date_1 TimeStamp -- DateTime
SET     @Date_1 = CAST('2009-09-20' AS timestamp)

SELECT
	"t"."BigIntValue",
	"t"."BoolValue",
	"t"."DateTimeValue",
	"t"."DateTimeValue2",
	"t"."GuidValue",
	"t".ID,
	"t"."IntValue",
	"t"."MoneyValue",
	"t"."SmallIntValue",
	"t"."StringValue"
FROM
	"LinqDataTypes" "t"
WHERE
	Cast("t"."DateTimeValue" as Date) > @Date_1

