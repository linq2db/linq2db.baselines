-- Firebird
DECLARE @p1 TimeStamp -- DateTime
SET     @p1 = CAST('2001-01-11 01:11:21.100' AS timestamp)

SELECT
	"t"."BigIntValue",
	"t"."BoolValue",
	"t"."DateTimeValue",
	"t"."DateTimeValue2",
	"t"."GuidValue",
	"t".ID,
	"t"."IntValue",
	"t"."MoneyValue",
	"t"."SmallIntValue",
	"t"."StringValue"
FROM
	"LinqDataTypes" "t"
WHERE
	"t"."DateTimeValue" IN (@p1)

