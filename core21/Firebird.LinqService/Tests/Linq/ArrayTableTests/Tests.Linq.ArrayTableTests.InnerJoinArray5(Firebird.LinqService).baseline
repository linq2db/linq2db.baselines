-- Firebird

SELECT
	"n"."c1",
	"p"."FirstName",
	"p"."Gender",
	"p"."PersonID",
	"p"."LastName",
	"p"."MiddleName"
FROM
	(
		SELECT
			'Janet' as "c1"
		FROM rdb$database
		UNION ALL
		SELECT
			'Doe' as "c1"
		FROM rdb$database
		UNION ALL
		SELECT
			'John' as "c1"
		FROM rdb$database
		UNION ALL
		SELECT
			'Doe' as "c1"
		FROM rdb$database
	) "n"
		INNER JOIN "Person" "p" ON "n"."c1" = "p"."LastName"

