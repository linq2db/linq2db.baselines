-- Firebird

SELECT
	"t1"."BigIntValue",
	"t1"."BoolValue",
	"t1"."DateTimeValue",
	"t1"."DateTimeValue2",
	"t1"."GuidValue",
	"t1".ID,
	"t1"."IntValue",
	"t1"."MoneyValue",
	"t1"."SmallIntValue",
	"t1"."StringValue"
FROM
	"LinqDataTypes" "t1"
WHERE
	(CASE
		WHEN "t1"."BoolValue" IN (1)
			THEN 1
		ELSE 0
	END = 0 OR CASE
		WHEN "t1"."BoolValue" IN (1)
			THEN 1
		ELSE 0
	END IS NULL)

