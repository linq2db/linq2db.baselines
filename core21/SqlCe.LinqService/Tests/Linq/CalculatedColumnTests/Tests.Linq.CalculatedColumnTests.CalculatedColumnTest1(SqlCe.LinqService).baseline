-- SqlCe

SELECT
	[i].[FirstName],
	[i].[Gender],
	[i].[LastName],
	[i].[MiddleName],
	[i].[PersonID],
	[i].[LastName] + ', ' + [i].[FirstName],
	[t1].[cnt]
FROM
	[Person] [i]
		LEFT JOIN (
			SELECT
				Count(*) as [cnt],
				[d].[PersonID]
			FROM
				[Doctor] [d]
			GROUP BY
				[d].[PersonID]
		) [t1] ON [t1].[PersonID] = [i].[PersonID]
WHERE
	[i].[FirstName] <> 'John'

