-- SqlCe
DECLARE @take Int -- Int32
SET     @take = 2

SELECT TOP (@take)
	[t].[BinaryValue],
	[t].[BoolValue],
	[t].[DateTimeValue],
	[t].[GuidValue],
	[t].[ID],
	[t].[MoneyValue],
	[t].[SmallIntValue],
	[t].[StringValue]
FROM
	[LinqDataTypes] [t]
WHERE
	[t].[MoneyValue] * Convert(Decimal(29,10), [t].[ID]) = 1.11

