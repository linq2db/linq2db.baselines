-- Firebird
DECLARE @take Integer -- Int32
SET     @take = 1

SELECT FIRST @take
	"t"."ChildID",
	"t"."GrandChildID",
	"t"."ParentID",
	"a_Child"."ChildID",
	"a_Child"."ParentID",
	"a_Child"."ParentID_1",
	"a_Child"."Value1"
FROM
	"GrandChild" "t"
		LEFT JOIN (
			SELECT
				"t1"."ChildID",
				"t1"."ParentID",
				"a_Parent"."ParentID" as "ParentID_1",
				"a_Parent"."Value1"
			FROM
				"Child" "t1"
					LEFT JOIN "Parent" "a_Parent" ON "t1"."ParentID" = "a_Parent"."ParentID"
		) "a_Child" ON "t"."ParentID" = "a_Child"."ParentID" AND "t"."ChildID" = "a_Child"."ChildID"

