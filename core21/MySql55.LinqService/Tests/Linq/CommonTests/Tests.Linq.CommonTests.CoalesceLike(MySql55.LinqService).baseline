SELECT
	`p`.`FirstName`,
	`p`.`Gender`,
	`p`.`PersonID`,
	`p`.`LastName`,
	`p`.`MiddleName`
FROM
	`Person` `p`
WHERE
	CASE
		WHEN CASE
			WHEN `p`.`FirstName` IS NULL
				THEN NULL
			ELSE `p`.`FirstName` LIKE 'Jo%'
		END IS NULL
			THEN 0
		WHEN `p`.`FirstName` IS NULL
			THEN NULL
		ELSE `p`.`FirstName` LIKE 'Jo%'
	END = 1

