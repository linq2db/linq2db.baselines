MERGE INTO TestMerge1 Target
USING (
	SELECT
		s.Id,
		s.Field1,
		s.Field2,
		s.Field3
	FROM
		TestMerge2 s
) Source
(
	Id, 
	Field1, 
	Field2, 
	Field3
)
ON (Source.Id = 3)

WHEN MATCHED THEN
UPDATE
SET
	Target.Field1 = Source.Field1,
	Target.Field2 = Source.Field2,
	Target.Field3 = Source.Field3

