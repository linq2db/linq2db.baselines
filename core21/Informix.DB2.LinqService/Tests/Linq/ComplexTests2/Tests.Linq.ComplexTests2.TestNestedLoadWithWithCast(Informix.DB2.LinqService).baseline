SELECT
	t2.Id,
	t2.TestAnimalId,
	a_TestAnimal.AnimalType,
	a_TestAnimal.AnimalType2,
	a_TestAnimal.Discriminator,
	a_TestAnimal.Id,
	a_TestAnimal.Name,
	a_TestAnimal.EyeId,
	a_TestAnimal.Second_1,
	a_TestAnimal.First_1,
	a_TestAnimal.Id_1,
	a_TestAnimal.Xy
FROM
	TestAnimalTable t2
		LEFT JOIN (
			SELECT
				t1.AnimalType,
				t1.AnimalType2,
				t1.Discriminator,
				t1.Id,
				t1.Name,
				t1.EyeId,
				t1."Second" as Second_1,
				t1."First" as First_1,
				o.Id as Id_1,
				o.Xy
			FROM
				Animals12 t1
					LEFT JOIN Eyes12 o ON t1.EyeId = o.Id AND t1.Discriminator = 'Dog'
		) a_TestAnimal ON t2.TestAnimalId = a_TestAnimal.Id
ORDER BY
	t2.Id

