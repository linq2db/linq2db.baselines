SELECT
	p.FirstName,
	p.Gender,
	p.PersonID,
	p.LastName,
	p.MiddleName
FROM
	Person p
WHERE
	CASE
		WHEN CASE
			WHEN p.FirstName IS NULL THEN NULL
			ELSE CASE
				WHEN p.FirstName LIKE 'Jo%'
					THEN 't'
				ELSE 'f'
			END
		END IS NULL
			THEN 'f'
		WHEN p.FirstName IS NULL THEN NULL
		ELSE CASE
			WHEN p.FirstName LIKE 'Jo%'
				THEN 't'
			ELSE 'f'
		END
	END = 't'

