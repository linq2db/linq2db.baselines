SELECT
	"a_Middle"."ChildID",
	"a_Middle"."GrandChildID",
	"a_Middle"."ParentID"
FROM
	"Parent" "t"
		LEFT JOIN (
			SELECT
				"a_Bottom"."ChildID",
				"a_Bottom"."GrandChildID",
				"a_Bottom"."ParentID",
				"t1"."ParentID" as "ParentID_1"
			FROM
				"Child" "t1"
					LEFT JOIN "GrandChild" "a_Bottom" ON "t1"."ChildID" = "a_Bottom"."ChildID"
		) "a_Middle" ON "t"."ParentID" = "a_Middle"."ParentID_1"
WHERE
	"t"."ParentID" IN (1, 5)
ORDER BY
	"t"."ParentID"

