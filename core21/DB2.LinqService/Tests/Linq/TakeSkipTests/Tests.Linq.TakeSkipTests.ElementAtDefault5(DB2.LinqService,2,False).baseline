-- DB2 DB2.LUW DB2LUW

SELECT
	"t2"."FirstName",
	"t2"."Gender",
	"t2"."PersonID",
	"t2"."LastName",
	"t2"."MiddleName"
FROM
	(
		SELECT
			"t1"."FirstName",
			"t1"."Gender",
			"t1"."PersonID",
			"t1"."LastName",
			"t1"."MiddleName",
			ROW_NUMBER() OVER (ORDER BY "t1"."LastName") as RN
		FROM
			"Person" "t1"
	) "t2"
WHERE
	"t2".RN > 2 AND "t2".RN <= 3

