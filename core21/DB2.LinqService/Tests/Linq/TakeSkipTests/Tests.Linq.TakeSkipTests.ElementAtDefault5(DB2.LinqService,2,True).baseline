-- DB2 DB2.LUW DB2LUW
DECLARE @skip Integer(4) -- Int32
SET     @skip = 2
DECLARE @skip_1 Integer(4) -- Int32
SET     @skip_1 = 3

SELECT
	"t2"."FirstName",
	"t2"."Gender",
	"t2"."PersonID",
	"t2"."LastName",
	"t2"."MiddleName"
FROM
	(
		SELECT
			"t1"."FirstName",
			"t1"."Gender",
			"t1"."PersonID",
			"t1"."LastName",
			"t1"."MiddleName",
			ROW_NUMBER() OVER (ORDER BY "t1"."LastName") as RN
		FROM
			"Person" "t1"
	) "t2"
WHERE
	"t2".RN > @skip AND "t2".RN <= @skip_1

