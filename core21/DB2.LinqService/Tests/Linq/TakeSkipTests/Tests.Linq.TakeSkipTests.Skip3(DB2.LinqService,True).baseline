-- DB2 DB2.LUW DB2LUW
DECLARE @skip Integer(4) -- Int32
SET     @skip = 3

SELECT
	"t1"."ChildID",
	"t1"."ParentID"
FROM
	(
		SELECT
			"ch"."ChildID",
			"ch"."ParentID",
			ROW_NUMBER() OVER () as RN
		FROM
			"Child" "ch"
		WHERE
			"ch"."ChildID" >= 0 AND "ch"."ChildID" <= 100
	) "t1"
WHERE
	"t1".RN > @skip

