-- DB2 DB2.LUW DB2LUW
DECLARE @skip Integer(4) -- Int32
SET     @skip = 3

SELECT
	"t2"."ChildID",
	"t2"."ParentID"
FROM
	(
		SELECT
			"t1"."ChildID",
			"t1"."ParentID",
			ROW_NUMBER() OVER (ORDER BY "t1"."ChildID" DESC, ("t1"."ParentID" + 1)) as RN
		FROM
			"Child" "t1"
	) "t2"
WHERE
	"t2".RN > @skip

