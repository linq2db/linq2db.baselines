SELECT
	"cp"."ParentID",
	"c_1"."ChildID"
FROM
	"Parent" "cp"
		INNER JOIN (
			SELECT DISTINCT
				"t3"."ChildID",
				"t3"."ParentID"
			FROM
				(
					SELECT
						"t1"."ChildID",
						"t1"."ParentID"
					FROM
						"Child" "t1"
					UNION
					SELECT
						"t2"."ChildID",
						"t2"."ParentID"
					FROM
						"Child" "t2"
				) "t3"
		) "c_1" ON "c_1"."ParentID" = "cp"."ParentID"
FETCH FIRST 10 ROWS ONLY

