-- SqlCe

SELECT
	[p].[FirstName],
	[p].[Gender],
	[p].[PersonID],
	[p].[LastName],
	[p].[MiddleName]
FROM
	[Person] [p]
WHERE
	CASE
		WHEN Len([p].[FirstName]) > 6
			THEN [p].[FirstName]
		ELSE [p].[FirstName] + Replicate(' ', 6 - Len([p].[FirstName]))
	END + '123' = 'John  123' AND
	[p].[PersonID] = 1

