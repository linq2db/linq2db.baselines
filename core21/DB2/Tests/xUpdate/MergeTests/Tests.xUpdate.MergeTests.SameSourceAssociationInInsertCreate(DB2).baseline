USING (
	SELECT
		"t1"."PersonID" as ID,
		"a_Patient"."Diagnosis"
	FROM
		"Person" "t1"
			LEFT JOIN "Patient" "a_Patient" ON "t1"."PersonID" = "a_Patient"."PersonID"
) "Source"
(
	ID, 
	"Diagnosis"
)
ON ("Target"."PersonID" = "Source".ID AND "Target"."FirstName" <> 'first 3')

