SELECT
	"key_data_result"."ParentID",
	"key_data_result"."Value1",
	"_ch"."ChildID",
	"_ch"."ParentID"
FROM
	(
		SELECT DISTINCT
			"p"."ParentID",
			"p"."Value1"
		FROM
			"Parent" "p"
		WHERE
			"p"."ParentID" >= 1
	) "key_data_result"
		INNER JOIN "Child" "_ch" ON "_ch"."ParentID" = "key_data_result"."ParentID"
ORDER BY
	"_ch"."ChildID"

