SELECT
	"t"."BinaryValue",
	"t"."BoolValue",
	"t"."DateTimeValue",
	"t"."GuidValue",
	"t".ID,
	"t"."MoneyValue",
	"t"."SmallIntValue",
	"t"."StringValue"
FROM
	"LinqDataTypes" "t"
WHERE
	"t"."BoolValue" = 0 AND (("t"."SmallIntValue" = 5 OR "t"."SmallIntValue" = 7) OR BitOr("t"."SmallIntValue", 2) = 10)

