-- DB2 DB2.LUW DB2LUW

SELECT
	"t2"."ChildID",
	"t2"."ParentID"
FROM
	(
		SELECT
			"t1"."ChildID",
			"t1"."ParentID",
			ROW_NUMBER() OVER (ORDER BY "t1"."ChildID" DESC) as RN
		FROM
			"Child" "t1"
	) "t2"
WHERE
	"t2".RN > 2 AND "t2".RN <= 7

