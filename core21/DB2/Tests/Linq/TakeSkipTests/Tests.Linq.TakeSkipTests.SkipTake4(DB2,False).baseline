SELECT
	"t4"."ChildID",
	"t4"."ParentID"
FROM
	(
		SELECT
			"t3"."ChildID",
			"t3"."ParentID",
			ROW_NUMBER() OVER (ORDER BY "t3"."ChildID") as RN
		FROM
			(
				SELECT
					"t2"."ChildID",
					"t2"."ParentID"
				FROM
					(
						SELECT
							"t1"."ChildID",
							"t1"."ParentID",
							ROW_NUMBER() OVER (ORDER BY "t1"."ChildID" DESC) as RN
						FROM
							"Child" "t1"
					) "t2"
				WHERE
					"t2".RN > 1 AND "t2".RN <= 8
			) "t3"
	) "t4"
WHERE
	"t4".RN > 2

