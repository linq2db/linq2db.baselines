SELECT
	`c_1`.`ChildID`,
	`c_1`.`GrandChildID`,
	`c_1`.`ParentID`
FROM
	`Child` `t`
		INNER JOIN `Parent` `a_Parent` ON `t`.`ParentID` = `a_Parent`.`ParentID`
		INNER JOIN `GrandChild` `c_1` ON `a_Parent`.`ParentID` = `c_1`.`ParentID`
WHERE
	`c_1`.`ParentID` = 1

