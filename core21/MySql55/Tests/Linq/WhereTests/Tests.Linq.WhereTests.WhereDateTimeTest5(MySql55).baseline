-- MySql55 MySql.Official MySql
DECLARE @Date_1 DateTime
SET     @Date_1 = '2009-09-20'

SELECT
	`_`.`BinaryValue`,
	`_`.`BoolValue`,
	`_`.`DateTimeValue`,
	`_`.`GuidValue`,
	`_`.`ID`,
	`_`.`MoneyValue`,
	`_`.`SmallIntValue`,
	`_`.`StringValue`
FROM
	`LinqDataTypes` `_`
WHERE
	Cast(`_`.`DateTimeValue` as Date) = @Date_1

