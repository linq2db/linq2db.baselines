-- Informix.DB2 Informix

SELECT
	p.FirstName,
	p.Gender,
	p.PersonID,
	p.LastName,
	p.MiddleName
FROM
	Person p
WHERE
	Substr(p.FirstName, 1, 2) || '123' || Substr(p.FirstName, Length(p.FirstName) - (Length(p.FirstName) - 3) + 1, Length(p.FirstName) - 3) = 'Jo123n' AND
	p.PersonID = 1

