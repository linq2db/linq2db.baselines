-- Informix.DB2 Informix

SELECT
	p1.FirstName,
	p1.Gender,
	p1.PersonID,
	p1.LastName,
	p1.MiddleName
FROM
	Person p1,
	Person p2
WHERE
	p1.PersonID = p2.PersonID AND Replace(p1.FirstName, 'J', '%') LIKE (Replace(Replace(Replace(Replace(p2.FirstName, 'J', '%'), '~', '~~'), '%', '~%'), '_', '~_') || '%') ESCAPE '~'

