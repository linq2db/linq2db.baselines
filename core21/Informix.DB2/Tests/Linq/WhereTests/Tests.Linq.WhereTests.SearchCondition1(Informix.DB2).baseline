-- Informix.DB2 Informix

SELECT
	t.BinaryValue,
	t.BoolValue,
	t.DateTimeValue,
	t.GuidValue,
	t.ID,
	t.MoneyValue,
	t.SmallIntValue,
	t.StringValue
FROM
	LinqDataTypes t
WHERE
	t.BoolValue = 'f' AND t.MoneyValue > 1 AND ((t.SmallIntValue = 5 OR t.SmallIntValue = 7) OR t.SmallIntValue = 8)

