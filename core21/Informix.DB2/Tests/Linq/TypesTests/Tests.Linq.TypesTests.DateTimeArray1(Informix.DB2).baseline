-- Informix.DB2 Informix
DECLARE @p1 Timestamp(16) -- DateTime
SET     @p1 = TO_DATE('2001-01-11 01:11:21.10000', '%Y-%m-%d %H:%M:%S.%F5')

SELECT
	t.BigIntValue,
	t.BoolValue,
	t.DateTimeValue,
	t.DateTimeValue2,
	t.GuidValue,
	t.ID,
	t.IntValue,
	t.MoneyValue,
	t.SmallIntValue,
	t.StringValue
FROM
	LinqDataTypes t
WHERE
	t.DateTimeValue IN (@p1)

