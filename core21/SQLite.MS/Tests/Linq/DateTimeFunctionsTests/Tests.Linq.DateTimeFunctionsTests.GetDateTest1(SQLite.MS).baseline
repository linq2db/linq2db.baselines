-- SQLite.MS SQLite
DECLARE @Date_1  -- DateTime
SET     @Date_1 = '2020-09-01'
DECLARE @take  -- Int32
SET     @take = 5

SELECT
	[t1].[c1],
	Count(*)
FROM
	(
		SELECT
			@Date_1 as [c1]
		FROM
			[Parent] [v]
				INNER JOIN [Child] [s] ON [v].[ParentID] = [s].[ParentID]
		WHERE
			[v].[Value1] > 0
	) [t1]
GROUP BY
	[t1].[c1]
LIMIT @take

