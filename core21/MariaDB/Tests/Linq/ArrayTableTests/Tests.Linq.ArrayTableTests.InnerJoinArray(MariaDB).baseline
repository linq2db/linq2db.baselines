	`p`.`FirstName`,
	`p`.`Gender`,
	`p`.`PersonID`,
	`p`.`LastName`,
	`p`.`MiddleName`
FROM
	`Person` `p`
		INNER JOIN (
			SELECT
				'Janet' as `c1`
			UNION ALL
			SELECT
				'Doe' as `c1`
			UNION ALL
			SELECT
				'John' as `c1`
			UNION ALL
			SELECT
				'Doe' as `c1`
		) `t1` ON `p`.`LastName` = `t1`.`c1`

