SELECT
	`p`.`FirstName`,
	`p`.`Gender`,
	`p`.`PersonID`,
	`p`.`LastName`,
	`p`.`MiddleName`
FROM
	`Person` `p`
WHERE
	`p`.`PersonID` = 1 AND CASE
		WHEN `p`.`MiddleName` IS NOT NULL
			THEN 3
		WHEN `p`.`MiddleName` IS NULL
			THEN 1
		ELSE 2
	END = 1 AND
	CASE
		WHEN `p`.`FirstName` IS NULL
			THEN 3
		WHEN `p`.`FirstName` IS NOT NULL
			THEN 1
		ELSE 2
	END = 1

