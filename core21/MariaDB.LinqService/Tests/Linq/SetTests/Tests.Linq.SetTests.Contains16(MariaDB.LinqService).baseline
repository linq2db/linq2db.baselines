SELECT
	`p`.`ChildID`,
	`p`.`GrandChildID`,
	`p`.`ParentID`
FROM
	`GrandChild` `p`
		LEFT JOIN `Child` `a_Child` ON `p`.`ParentID` = `a_Child`.`ParentID` AND `p`.`ChildID` = `a_Child`.`ChildID`
WHERE
	(`a_Child`.`ChildID` = 11 AND `a_Child`.`ParentID` = 1 OR `a_Child`.`ChildID` = 21 AND `a_Child`.`ParentID` = 2)

