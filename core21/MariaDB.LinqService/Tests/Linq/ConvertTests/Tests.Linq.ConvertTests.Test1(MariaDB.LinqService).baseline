-- MariaDB MySql.Official MySql
DECLARE @take Int32
SET     @take = 2

SELECT
	`t`.`BinaryValue`,
	`t`.`BoolValue`,
	`t`.`DateTimeValue`,
	`t`.`GuidValue`,
	`t`.`ID`,
	`t`.`MoneyValue`,
	`t`.`SmallIntValue`,
	`t`.`StringValue`
FROM
	`LinqDataTypes` `t`
WHERE
	`t`.`MoneyValue` * Cast(`t`.`ID` as DECIMAL(29,10)) = 1.11
LIMIT @take

